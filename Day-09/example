# tuple
tuple = ("apple", "banana", "cherry")
for i in tuple:
    print(i)


# range
print("range")
for i in range(10):
    print(i)


# break 
print("break")
for i in range(10):
    print(i)
    if i == 5:
        break
        print(i)
# continous
print("continue")
for i in range(10):
    print(i)
    if i == 5:
        continue
        print(i)



